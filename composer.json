{
    "name": "casperwilkes/laravel-environment_detector2",
    "description": "Builds appropriate .env configs, detects current laravel environment, uses corresponding .env. Requires Laravel Framework 7+.",
    "keywords": [
        "environment",
        "detector",
        "laravel",
        "multiple",
        "multiple environments",
        "hosts",
        "configs"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Casper Wilkes",
            "email": "casper@casperwilkes.net",
            "homepage": "https://casperwilkes.net",
            "role": "Software Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2.5",
        "illuminate/console": "^7.0",
        "illuminate/support": "^7.0",
        "laravel/framework": "^7.0",
        "vlucas/phpdotenv": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "EnvDetector\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EnvDetector\\EnvDetectorServiceProvider"
            ]
        }
    }
}
